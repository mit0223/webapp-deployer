# AWS アカウント設定
AWS_ACCOUNT_ID=
AWS_REGION=ap-northeast-1

# IAMロール設定
CDK_DEPLOYER_ROLE_NAME=CdkDeployer

# MFA設定
MFA_DEVICE_ARN=arn:aws:iam::123456789012:mfa/username

# AWS認証情報（IAMユーザー）
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# ECSアプリケーション設定
APP_NAME=webapp
CONTAINER_PORT=8080
CONTAINER_IMAGE=ghcr.io/yourorg/webapp:latest
DESIRED_COUNT=2
CPU=256
MEMORY=512

# VPC設定
VPC_CIDR=10.0.0.0/16
PUBLIC_SUBNET_MASK=24
PRIVATE_SUBNET_MASK=24
NAT_GATEWAYS=1

# ALB設定
ALB_PORT=80
HEALTH_CHECK_PATH=/health
